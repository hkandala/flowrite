{
  "name": "flowrite",
  "description": "a markdown editor, reimagined for the age of ai agents",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "tauri": "tauri",
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "pnpm format:frontend && pnpm format:rust",
    "format:frontend": "prettier --write .",
    "format:rust": "cd src-tauri && cargo fmt",
    "update-deps": "pnpm update-deps:frontend && pnpm update-deps:rust",
    "update-deps:frontend": "pnpm update",
    "update-deps:rust": "cd src-tauri && cargo update",
    "precommit": "pnpm update-deps && pnpm format"
